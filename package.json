{
  "name": "webpack-typescript-babel",
  "version": "1.0.0",
  "description": "A sample setup using Webpack and Babel to build TypeScript code, and using TypeScript for type-checking.",
  "scripts": {
    "type-check": "npx tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "build:types": "npx tsc --emitDeclarationOnly",
    "build:ts": "npx tsc -p .",
    "build:js": "npx gulp default",
    "build:jses5": "npx gulp es5",
    "clean": "npx gulp clean",
    "build": "npx gulp",
    "start": "webpack-dev-server --mode=development"
  },
  "author": {
    "name": "Tiberiu  Corbu",
    "email": "tiberiu.corbu@gmail.com"
  },
  "main": "dist/bundle.js",
  "types": "dist/types/index.d.ts",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "7.11.1",
    "@babel/plugin-proposal-class-properties": "7.10.4",
    "@babel/plugin-proposal-decorators": "7.10.5",
    "@babel/plugin-proposal-object-rest-spread": "7.11.0",
    "@babel/polyfill": "7.10.4",
    "@babel/preset-env": "7.11.0",
    "@babel/preset-typescript": "7.10.4",
    "babel-loader": "8.1.0",
    "core-js": "3.6.5",
    "del": "5.1.0",
    "fork-ts-checker-webpack-plugin": "5.0.14",
    "gulp": "4.0.2",
    "gulp-babel": "8.0.0",
    "gulp-rename": "2.0.0",
    "gulp-replace": "1.0.0",
    "html-webpack-plugin": "4.3.0",
    "ts-loader": "8.0.2",
    "typescript": "3.9.7",
    "webpack": "^5.64.1",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.5.0",
    "webpack-stream": "^7.0.0"
  },
  "resolutions": {
    "terser": "3.14.1"
  },
  "dependencies": {
    "@webcomponents/webcomponentsjs": "2.4.4",
    "lit-element": "^2.3.1"
  }
}
